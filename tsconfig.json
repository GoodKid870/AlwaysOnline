{
    "compilerOptions": {
        "skipLibCheck": true,
        "importHelpers": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "moduleResolution": "node",
        "target": "esnext",
        "module": "commonjs",
        "lib": ["esnext"]
    },
    "exclude": ["node_modules"]
}